page{
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
}

.wraper{
  width: 100%;
  height: 100%;
  font-size: 14px;
  color: #FFF;
}

.memberRanking-title{
  padding: 10px 0;
  overflow: hidden;
  border-bottom: 1px solid #F0F0F2;
}

.memberRanking-title > view{
  float: left;
  width: 33.33%;
  text-align: center;
  color: #444;
}

.memberRanking-title :first-child{
  text-align: left;
}

.memberRanking-title :last-child{
  text-align: right;
}

.no-data{
  position: absolute;
  top: 40%;
  left: 40%;
  text-align: center;
  font-size: 12px;
  color: #8f8e8e;
}

.no-data > image{
  width: 30px;
  height: 30px;
}

.memberRanking-item{
  padding: 10px 0;
  overflow: hidden;
  border-bottom: 1px solid #F0F0F2;
}

.memberRanking-item > view{
  float: left;
  width: 33.33%;
  color: #444;
}

.ranking{
  height: 30px;
  line-height: 30px;
  font-size: 12px;
}

.ranking > image{
  margin-left: 12px;
  width: 27px;
  height: 30px;
}

.memberInfo{
  overflow: hidden;
}

.memberInfo-image{
  float: left;
  margin-left: 20px;
  width: 30px;
  height: 30px;
  border-radius: 50%;
}

.memberInfo-name{
  float: left;
  margin-left: 10px;
  width: 56px;
  height: 30px;
  line-height: 30px;
  overflow: hidden;
}

.count{
  height: 30px;
  text-align: right;
  line-height: 30px;
}

.count > text{
  color: #BBB;
}

.goHome{
  position: fixed;
  bottom: 80px;
  right: 0;
  z-index: 1;
  padding: 5px;
  background-color: rgba(0, 0, 0, 0.5);
}

.goHome > image{
  margin-left: 5px;
  width: 30px;
  height: 30px;
}

.goHome > view{
  text-align: center;
  font-size: 10px;
  color: #FFF;
}

