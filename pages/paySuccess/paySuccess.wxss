.paySuccess-icon{
  margin-top: 100px;
  text-align: center;
}

.paySuccess-icon > image{
  width: 120px;
  height: 120px;
}

.paySuccess-title{
  margin-top: 10px;
  text-align: center;
  font-size: 18px;
  color: #1e1e1e;
  font-weight: 400;
}

.paySuccess-content{
  margin-top: 20px;
  text-align: center;
  font-size: 12px;
  color: #999;
}

.button-tip{
  margin: 0 auto;
  margin-top: 20px;
  width: 231px;
  height: 33px;
  background-color: #FF9800;
  text-align: center;
  line-height: 33px;
  font-size: 14px;
  color: #FFF;
  border-radius: 20px;
}

.paySuccess-message{
  padding: 10px;
  text-align: center;
  font-size: 12px;
  color: #999;
}

.paySuccess-hr{
  margin: 20px auto;
  width: 80%;
  height: 1px;
  background-color: #DCDCDC; 
}