@import "../../wxParse/wxParse.wxss";
page{
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;
  font-size: 14px;
  color: #FFF;
}

.project-item{
  padding: 10px;
  overflow: hidden;
}

.project-image{
  float: left;
}

.project-image > image{
  width: 114px;
  height: 68px;
}

.project-name{
  text-align: center;
  color: #444;
}

.project-content{
  float: right;
  width: 60%;
  color: #BBB;
  font-size: 12px;
}