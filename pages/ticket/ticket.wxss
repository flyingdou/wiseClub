page{
  width: 100%;
  height: 100%;
}

.wraper{
  position: relative;
  width: 100%;
  height: 100%;
  background-color: #F2F2F2;
}

.no-data{
  position: absolute;
  top: 40%;
  left: 40%;
  text-align: center;
  font-size: 12px;
  color: #8f8e8e;
}

.no-data > image{
  width: 30px;
  height: 30px;
}

.no-data-left{
  left: 30%;
}

.ticket-list{
  padding: 10px;
}

.ticket-li{
  overflow: hidden;
}

.ticket-content{
  float: left;
  width: 80%;
  height: 90px;
  background-color: #FFF;
  margin-bottom: 20rpx;
}

.ticket-content-wraper{
  padding: 10px;
}

.ticket-name{
  font-size: 16px;
}

.ticket-type{
  margin-top: 10px;
  font-size: 12px;
  color: #8f8e8e;
}

.limitPrice{
  margin-left: 10px;
  font-size: 12px;
  color: #8f8e8e;
}

.ticket-validity{
  margin-top: 10px;
  font-size: 14px;
  color: #8f8e8e;
}

.ticket-price{
  position: relative;
  float: right;
  width: 20%;
  height: 90px;
  line-height: 90px;
  text-align: center;
  background-color: #FF6600;
  font-size: 16px;
  color: #FFF;
}

.ticket-price > image{
  width: 100%;
  height: 100%;
}

.price-text{
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
  width: 100%;
  text-align: center;
  font-size: 16px;
  color: #FFF;
}

.RMB{
    font-size: 22rpx;
    color: white;
}

.activeTicket-div{
   position: fixed;
   width: 100%;
   left: 0;
   bottom: 0;

}

.goNow{
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 24px;
  line-height: 24px;
  text-align: center;
  font-size: 12px;
}

.activeTicket{
   margin: 0 auto;
   height: 44px;
   line-height: 44px;
   text-align: center;
   color: white;
   font-size: 32rpx;
   background: #FF9800;
}